    "lista = []\n",
    "numero = 0 \n",
    "while numero == 10000:\n",
    "    numero = int(input(\"Digite um número para adicionar à lista: \"))\n",
    "    lista.append(numero)\n",
    "\n",
    "print(\"Lista preenchida:\", lista)\n"
