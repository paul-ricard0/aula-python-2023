    "# 17) Fazer a média de uma lista\n",
    "def media_lista(lista):\n",
    "    soma = sum(lista)\n",
    "    media = soma / len(lista)\n",
    "    return media\n",
    "\n",
    "lista = [10, 20, 30, 40, 50]\n",
    "resultado = media_lista(lista)\n",
    "print(f\"A média da lista é: {resultado}\")"
   ]
