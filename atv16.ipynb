    "# 16) Somar todos os valores de uma lista\n",
    "def soma_lista(lista):\n",
    "    soma = 0\n",
    "    for numero in lista:\n",
    "        soma += numero\n",
    "    return soma\n",
    "\n",
    "lista = [1, 2, 3, 4, 5]\n",
    "resultado = soma_lista(lista)\n",
    "print(f\"A soma da lista Ã©: {resultado}\")"
